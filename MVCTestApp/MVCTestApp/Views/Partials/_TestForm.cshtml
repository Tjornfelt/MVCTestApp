@model MVCTestApp.Models.TestForm


@using (Ajax.BeginForm("TestFormAction", "TestForm", null, new AjaxOptions()
{
    OnComplete = "alert(\"TEST!\");"
}, null))
{
    @Html.LabelFor(x => x.String)
    @Html.EditorFor(x => x.String)
    @Html.ValidationMessageFor(x => x.String)
